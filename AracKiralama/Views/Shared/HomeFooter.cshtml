@model AracKiralama.Models.Sınıflar.HomePageDatas

<!-- Footer Starts Here -->
<footer>
    <div class="container">
        <div class="row">
            <div class="col-md-4 footer-item">
                @foreach (var set in Model.Settings)
                {
                    <h4>@set.Title Website</h4>
                    <p>@Html.Raw(set.Contactus)</p>
                    <ul class="social-icons">

                        <li><a rel="nofollow" href="@set.Facebook" target="_blank"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="@set.Twitter" target="_blank"><i class="fa fa-twitter"></i></a></li>
                        <li><a href="@set.Instagram" target="_blank"><i class="fa fa-instagram"></i></a></li>



                    </ul>
                }
            </div>
            <div class="col-md-4 footer-item">
                <h4>Kategoriler</h4>
                <ul class="menu-list">
                    @foreach (var ktgr in Model.Kategoris)
                    {
                        <li><a href="/Home/KategoriArabalar/@ktgr.KategoriId">@ktgr.Title</a></li>

                    }
                </ul>
            </div>
            <div class="col-md-4 footer-item">
                <h4>Hızlı Ulaş</h4>
                <ul class="menu-list">
                    <li><a href="/Home/Hakkımızda">About Us</a></li>
                    <li><a href="/Home/İletişim">İletişim</a></li>
                    <li><a href="/Home/Referanslarımız">Contact Us</a></li>
                    <li><a href="#">FAQ</a></li>

                </ul>
            </div>

        </div>
    </div>
</footer>

<div class="sub-footer">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <p>
                    Copyright © 2020 Company Name
                    - Template by: <a href="https://www.phpjabbers.com/">PHPJabbers.com</a>
                </p>
            </div>
        </div>
    </div>
</div>

